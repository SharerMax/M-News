<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="WeiboAccessStyle" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="colorPrimary">@color/green_500</item>
        <item name="colorPrimaryDark">@color/green_800</item>
    </style>

    <style name="fab_menu_labels_style">
        <item name="android:background">@drawable/fab_lable</item>
        <item name="android:textColor">@color/white</item>
    </style>

    <!-- ScrimInsetLayout -->
    <style name="ScrimInsetsLayout">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:fitsSystemWindows">true</item>
        <item name="android:layout_gravity">start</item>
        <item name="insetForeground">@color/nav_drawer_inset_foreground</item>
    </style>
</resources>
